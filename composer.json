{
    "name": "zfegg/symfony-twig-factory",
    "description": "Using symfony/twig-bridge in Mezzio. (e.g.: asset,workflow)",
    "type": "library",
    "keywords": [
        "mezzio",
        "laminas",
        "expressive",
        "twig",
        "symfony",
        "asset",
        "factory"
    ],
    "require": {
        "php": ">=7.3",
        "symfony/twig-bridge": "^4.0 | ^5.0 | ^6.0",
        "psr/container": "^1.0 | ^2.0"
    },
    "require-dev": {
        "mezzio/mezzio-twigrenderer": "^2.3",
        "laminas/laminas-coding-standard": "^2.3.0",
        "laminas/laminas-servicemanager": "^3.3",
        "phpunit/phpunit": "9.5.16",
        "phpspec/prophecy-phpunit": "^2.0",
        "symfony/asset": "^4.0 | ^5.0 | ^6.0",
        "symfony/expression-language": "^4.2 | ^5.0",
        "symfony/form": "^4.0.9 | ^5.0 | ^6.0",
        "symfony/workflow": "^4.0 | ^5.0 | ^6.0",
        "symfony/yaml": "^4.0 | ^5.0 | ^6.0",
        "symfony/security-csrf": "^4.2 | ^5.0 | ^6.0",
        "symfony/translation": "^4.2 | ^5.0 | ^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Moln",
            "email": "moln.xie@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zfegg\\SymfonyTwigFactory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZfeggTest\\SymfonyTwigFactory\\": "test/"
        }
    },
    "suggest": {
        "symfony/asset": "Asset factory required"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
